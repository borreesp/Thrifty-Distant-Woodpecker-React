{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/.next/**",
        "apps/mobile/dist/**",
        "packages/**/dist/**"
      ]
    },
    "lint": {
      "outputs": []
    }
  }
}
